{
  "version": 2,
  "name": "fasttrack-drop-taxi-backend",
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "https://comforting-douhua-1da089.netlify.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      },
      "dest": "/"
    }
  ]
}